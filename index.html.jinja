  <main>
    <section id="bio">
      <header>
        <h1>{{ basics.name }}</h1>
      </header>
      <ul>
        <li>
          <img src="{{ basics.image }}">
        </li>
        <li>{{ basics.label }}</li>
        <li><a href="https://psych.princeton.edu">{{ education[0].area }}</a></li>
        <li><a href="{{ education[0].url }}">{{ education[0].institution }}</a></li>

      </ul>
      <p>{{ basics.summary }}</p>
      <footer>
        {% for profile in basics.profiles %}
        <a href="{{ profile.url }}">
          <svg class="icon icon-{{ profile.network }}" width="32" height="32">
            <use xlink:href="#icon-{{ profile.network }}"></use>
          </svg>
        </a>
        {% endfor %}
      </footer>
    </section>
    <section id="papers">
      <h1>Papers</h1>
      {% for paper in publications %}
      <article>
        <header>
          <a href="{{ paper.url }}">{{ paper.name }}</a>
        </header>
        <p>{{ paper.authors }}</p>
        <footer>
          {% if paper.publisher.url is defined %}
          <a href="{{ paper.publisher.url }}"><span class="journal_badge">{{ paper.publisher.name }}</span></a>
          {% endif %}
          {% if paper.preprint is defined %}
          <a href="{{ paper.preprint.url }}"><span class="preprint_badge">{{ paper.preprint.name }} {{
              paper.releaseDate[:4] }}</span></a>
          {% endif %}
          {% if paper.code is defined %}
          <a href="{{ paper.code }}"><span class="code_badge">code</span></a>
          {% endif %}
        </footer>
      </article>
      {% endfor %}
    </section>

    <section id="talks">
      <h1>Presentations</h1>
      <ul>
        {% for talk in presentations %}
        <li>
          <time datetime="{{ talk.date }}">{{ talk.date.strftime('%m/%Y') }}</time>
          {{ talk.name }} at {{ talk.venue }}.
          {% if talk.url is defined %}
          <a href="{{ talk.url }}"><span class="border_badge">{{ talk.type }}</span></a>
          {% else %}
          <span class="border_badge">{{ talk.type }}</span>
          {% endif %}
          <br>
        </li>
        {% endfor %}
      </ul>
    </section>

    <section id="awards">
      <h1>Awards</h1>
      <ul>
        {% for award in awards %}
        <li>
          <time datetime="{{ award.date }}">{{ award.date.strftime('%Y') }}</time>
          {% if award.url is defined %}
          <a href="{{ award.url }}">{{ award.title }}</a>
          {% else %}
          {{ award.title }}
          {% endif %}
          from {{ award.awarder }}.
          <br>
        </li>
        {% endfor %}
      </ul>
    </section>

    <section id='software'>
      <h1>Software</h1>
      <h2>macOS</h2>
      <ul>
        {% for project in projects %}
        <li>
          <a href="{{ project.url }}">
            <svg class=" icon icon-{{ project.name }}" width="64" height="64">
              <use xlink:href="#icon-{{ project.name }}"></use>
            </svg>
          </a>
          <p><strong>{{project.name}}</strong>. {{ project.summary }}</p>
        </li>
        {% endfor %}
      </ul>
    </section>

    <section id="media">
      <h1>Media</h1>
      <ul>
        {% for article in media %}
        <li><a href="{{ article.url }}">{{ article.name }}</a></li>
        {% endfor %}
      </ul>
    </section>

  </main>

  <footer>
    <p>&copy; {{ basics.name }} <time datetime="2023-10-28">2023</time></p>
  </footer>
</body>

</html>